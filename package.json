{
  "name": "@datadog/pprof",
  "version": "0.2.0",
  "description": "pprof support for Node.js",
  "repository": "datadog/pprof-nodejs",
  "main": "out/src/index.js",
  "types": "out/src/index.d.ts",
  "scripts": {
    "preinstall": "node scripts/preinstall.js",
    "install": "(node scripts/should_rebuild && node-gyp-build) || exit 0",
    "rebuild": "node-gyp rebuild --jobs=max",
    "test": "npm run test:cpp && npm run test:js",
    "test:js": "nyc mocha out/test/test-*.js",
    "test:cpp": "node scripts/cctest.js",
    "check:cpp": "cppcheck --enable=information,performance,portability,style,warning --suppress=missingIncludeSystem --suppress=missingInclude --std=c++14 ./bindings",
    "codecov": "nyc report --reporter=json && codecov -f coverage/*.json",
    "build:js": "tsc -p .",
    "lint": "npm run lint:cpp && npm run lint:js",
    "lint:cpp": "cpplint --recursive --filter=-build/include_subdir,-legal/copyright ./bindings",
    "lint:js": "eslint .",
    "prebuild": "node scripts/prebuild.js",
    "prebuilds": "node scripts/prebuilds.js",
    "prepare": "npm run build:js",
    "prepublishOnly": "node scripts/prepublish.js",
    "pretest": "npm run build:js && npm run rebuild",
    "licenses": "node scripts/check_licenses.js"
  },
  "author": "Datadog Inc. <info@datadoghq.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/DataDog/pprof-nodejs/issues"
  },
  "homepage": "https://github.com/DataDog/pprof-nodejs#readme",
  "dependencies": {
    "delay": "^5.0.0",
    "rimraf": "^3.0.2",
    "split": "^1.0.1"
  },
  "devDependencies": {
    "@types/mocha": "^8.0.0",
    "@types/node": "^15.6.1",
    "@types/pretty-ms": "^4.0.0",
    "@types/request": "^2.47.1",
    "@types/tmp": "0.2.1",
    "axios": "^0.21.1",
    "checksum": "^0.1.1",
    "codecov": "^3.0.0",
    "eslint": "^7.31.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "glob": "^7.1.7",
    "mkdirp": "^1.0.4",
    "mocha": "^8.0.0",
    "node-addon-api": "^4.1.0",
    "node-gyp-build": "^3.9.0",
    "nyc": "^15.0.0",
    "rimraf": "^3.0.2",
    "semver": "^7.3.5",
    "tar": "^6.1.0",
    "tmp": "0.2.1",
    "typescript": "~4.3.0"
  },
  "files": [
    "out/src",
    "out/third_party/cloud-debug-nodejs",
    "bindings",
    "proto",
    "binding.gyp",
    "LICENSE-3rdparty.csv",
    "package.json",
    "prebuilds",
    "README.md",
    "scripts/preinstall.js",
    "scripts/require-package-json.js",
    "scripts/should_rebuild.js"
  ],
  "nyc": {
    "exclude": [
      "out/test"
    ]
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
